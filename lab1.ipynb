    "def isPrime(n):\n",
    "    if n % 2 == 0:\n",
    "        return n == 2\n",
    "    k = 3\n",
    "    while k * k <= n and n % k != 0:\n",
    "        k += 2\n",
    "    return k * k > n"
   ],
